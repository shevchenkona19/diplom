		"bcrypt": "^4.0.1",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.20.2",
		"express": "^4.17.1",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"pg": "^7.18.2",
		"pg-hstore": "^2.3.3",
		"sequelize": "^5.21.5",
		"ts-node": "^8.8.1",
		"tsconfig-paths": "^3.9.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.0",
