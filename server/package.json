	"description": "This is the server for my diploma work",
	"main": "/src/index.ts",
	"author": "Nikita Shevchenko",
	"license": "MIT",
	"scripts": {
		"start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
		"start:watch": "nodemon -r dotenv/config",
		"build": "tsc",
		"test": "tsc && node ./dist/controllers/clustering.js"
	},
	"nodemonConfig": {
		"ignore": [
			"**/*.test.ts",
			"**/*.spec.ts",
			".git",
			"node_modules"
		],
		"watch": [
			"src"
		],
		"exec": "npm start",
		"ext": "ts"
	},
	"dependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/bluebird": "^3.5.30",
